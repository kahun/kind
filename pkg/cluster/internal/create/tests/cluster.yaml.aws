cluster_id: fran-capi-eks
k8s_version: v1.23.0
region: eu-west-1
infra_provider: aws
external_registry:
    auth_required: false
    type: docker
    url: registry
keos:
  version: 0.8.0
  domain: cluster.local
  external_domain: domain.ext
  flavour: development
networks:
  vpc_id: vpc-0425c335226437144
  subnets:
    - name: subnet-0261219d564bb0dc5
    - name: subnet-0fdcccba78668e013
control_plane:
  managed: false
  size: t3.medium
worker_nodes:
  - name: workers
    quantity: 3
    size: t3.2xlarge
    root_volume:
      size: 30
      type: gp2
      encrypted: true
  - name: gpu
    quantity: 2
    size: t3.2xlarge
    az: eu-west-1a
