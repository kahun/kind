cluster_id: fran-capi-eks
k8s_version: v1.23.0
region: europe-west4
infra_provider: gcp
external_registry:
    auth_required: false
    type: docker
    url: registry
keos:
  version: 0.8.0
  domain: cluster.local
  external_domain: domain.ext
  flavour: development
control_plane:
  managed: false
  size: n1-highcpu-4
worker_nodes:
  - name: workers
    quantity: 3
    size: c2d-highcpu-4
    root_volume:
      size: 30
      type: gp2
